<center>
	<b><i><h1>Costos Atenciones</h1></i></b>
	<form action="/controladorUsuario" method="post">
	<table border="2" bgcolor="white">
	<tr>
		<td><center>PERROS</center></td>
		<td><center>GATOS</center></td>
	</tr>
	<tr>
		<td><center>ASEO</center></td>
		<td><center></center></td>
	</tr>
	<tr>
		<td><center>S/. 40.00 <input type="checkbox" name="costo" value="a1"></center></td>
		<td><center>S/. 25.00 <input type="checkbox" name="costo" value="a2"></center></td>
	</tr>
	<tr>
		<td><center>CAMBIO ESTILO</center></td>
		<td><center></center></td>
	</tr>
	<tr>
		<td><center>S/. 30.00 <input type="checkbox" name="costo" value="a3"></center></td>
		<td><center>S/. 15.00 <input type="checkbox" name="costo" value="a4"></center></td>
	</tr>
	<tr>
		<td><center>SPA</center></td>
		<td><center></center></td>
	</tr>
	<tr>
		<td><center>S/. 50.00 <input type="checkbox" name="costo" value="a5"></center></td>
		<td><center>S/. 40.00 <input type="checkbox" name="costo" value="a6"></center></td>
	</tr>


	</table>
	<input type="submit" name="enviarUsuario" value="Costos">
	</form>
<% 
double totalA=0;
double v1=0;  double v2=0;  double v3=0;  double v4=0;  double v5=0;  double v6=0;
String[] costo=request.getParameterValues("costo");
if(costo!= null){
    for (int i=0;i<costo.length;i++){
        if(costo[i].matches("a1")){
            v1=40;
        }
        if(costo[i].matches("a2")){
            v2=25;
        }
        if(costo[i].matches("a3")){
            v3=30;
        }
        if(costo[i].matches("a4")){
            v4=15;
        }
        if(costo[i].matches("a5")){
            v5=50;
        }
        if(costo[i].matches("a6")){
            v6=40;
        }
    }
    totalA=v1+v2+v3+v4+v5+v6;
    
    %>
    <br>
    <table bgcolor="white"><tr><td><h1>El total de lo que quiere comprar es : S/.<% out.println(totalA);%></h1></td></tr></table>
    <br><br>
    <%
}
%>

        
